# AINEON Enterprise Configuration Example
# Copy this file to .env and fill in your values
# NEVER commit .env to Git!

# Ethereum RPC Endpoint
# Use Alchemy (https://www.alchemy.com) or Infura (https://www.infura.io)
# For testing: Use Sepolia testnet
# For production: Use mainnet
ETH_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Smart Contract Address
# Deploy or use existing contract address
# Example (testnet): 0x0000000000000000000000000000000000000000
CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Wallet Address for Trading
# Your Ethereum wallet address that will execute trades
# Must have sufficient balance for gas and trading capital
WALLET_ADDRESS=0x0000000000000000000000000000000000000000

# Private Key for Transaction Signing
# Your private key (64 hex characters, starts with 0x)
# KEEP THIS SECURE - NEVER SHARE!
# Leave empty for monitoring-only mode
PRIVATE_KEY=

# Gasless Mode Configuration (Optional)
# Pimlico Paymaster for ERC-4337 gasless transactions
PAYMASTER_URL=

# Pimlico Bundler for gasless mode
BUNDLER_URL=

# Server Port
# Default: 8081
PORT=8081

# Logging Level
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Maximum Daily Loss (in USD)
# Default: 1,500,000 (triggers circuit breaker)
MAX_DAILY_LOSS=1500000

# Maximum Position Size (in USD)
# Default: 10,000,000 (max per trade)
MAX_POSITION_SIZE=10000000

# Trading Capital (in ETH)
# Starting capital for the system
TRADING_CAPITAL=1.0

# Profit Transfer Mode
# Options: MANUAL (default), AUTO, DISABLED
# MANUAL: Profits accumulate, transfer requires manual action
# AUTO: Profits automatically transfer at threshold
# DISABLED: Profits accumulate indefinitely
TRANSFER_MODE=MANUAL

# Auto-Transfer Recipient Address (for AUTO mode)
# Where to send profits when auto-transfer is enabled
AUTO_TRANSFER_ADDRESS=0x0000000000000000000000000000000000000000

# Auto-Transfer Threshold (in ETH)
# Only used if TRANSFER_MODE=AUTO
# Transfer when accumulated ETH exceeds this amount
AUTO_TRANSFER_THRESHOLD=10.0

# Etherscan API Key (for transaction verification)
ETHERSCAN_API_KEY=

# Environment
NODE_ENV=development