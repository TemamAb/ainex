# Flashbots Auth Key (generate with: openssl rand -hex 32)
FLASHBOTS_AUTH_KEY=

# ============================================
# AI OPTIMIZATION (Optional)
# ============================================

# Google Gemini API Key (for AI copilot)
VITE_GEMINI_API_KEY=

# ============================================
# TRADING PARAMETERS
# ============================================

# Minimum profit threshold (in ETH)
MIN_PROFIT_THRESHOLD=0.001

# Maximum slippage tolerance (percentage)
MAX_SLIPPAGE=0.5

# Gas price limit (in gwei)
MAX_GAS_PRICE=100

# ============================================
# MONITORING & ALERTS
# ============================================

# Enable/Disable LIVE mode
VITE_ENABLE_LIVE_MODE=false

# Node environment
NODE_ENV=production

# ============================================
# SECURITY NOTES
# ============================================
# 1. NEVER commit this file to git
# 2. Keep PRIVATE_KEY secret - it controls your funds
# 3. Use separate wallets for deployment and trading
# 4. Start with small amounts in LIVE mode
# 5. Monitor gas prices - high gas can make trades unprofitable
