# ============================================
# AINEX PRODUCTION ENVIRONMENT CONFIGURATION
# ============================================
# Copy this to .env and fill in your actual values

# ============================================
# BLOCKCHAIN RPC ENDPOINTS (REQUIRED)
# ============================================

# Alchemy Mainnet URL (Get from: https://www.alchemy.com/)
ALCHEMY_MAINNET_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY_HERE

# Alternative: Infura (if not using Alchemy)
# INFURA_MAINNET_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID

# Ethereum RPC (fallback)
ETH_RPC_URL=${ALCHEMY_MAINNET_URL}

# ============================================
# WALLET CONFIGURATION (REQUIRED)
# ============================================

# Deployer Private Key (KEEP SECRET!)
# This wallet needs ~0.1 ETH for gas
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Frontend Wallet Address (for dashboard)
VITE_WALLET_ADDRESS=0xYOUR_WALLET_ADDRESS_HERE

# ============================================
# DEPLOYED CONTRACT ADDRESSES
# ============================================
# Fill these in after deployment

# ApexDEXRouter contract address
DEX_ROUTER_ADDRESS=

# ApexFlashAggregator contract address
FLASH_AGGREGATOR_ADDRESS=

# ============================================
# FLASH LOAN PROVIDER ADDRESSES (Mainnet)
# ============================================

# Aave V3 Pool Address Provider
